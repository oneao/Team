<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.oneao.project.mapper.PjBugMapper">
    <delete id="realDeleteByProjectIds">
        delete from pj_bug
        where pj_project_id in
        <foreach collection="ids" item="projectId" open="(" close=")" separator=",">
            #{projectId}
        </foreach>
    </delete>
</mapper>