<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.oneao.project.mapper.PjTaskLikeMapper">


    <delete id="realDeleteByTaskIds">
        delete from pj_task_like where pj_task_id in
        <foreach collection="taskIds" item="item" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </delete>
    <select id="selectLikeCountByTaskId" resultType="java.lang.Long">
        select count(1) from pj_task_like where pj_task_id = #{id}
    </select>
</mapper>